{
  "name": "@datawheel/tesseract-vizbuilder",
  "version": "0.4.1",
  "description": "Generates a vizbuilder view for results query in Tesseract Explorer.",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "default": "./dist/index.cjs.js"
    },
    "./vizbuilder.css": "./dist/index.css"
  },
  "types": "./dist/index.d.ts",
  "style": "./dist/index.css",
  "readme": "README.md",
  "files": [
    "dist/index.cjs.js",
    "dist/index.css",
    "dist/index.d.ts",
    "dist/index.esm.js"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "tsup"
  },
  "keywords": [
    "charts",
    "d3plus",
    "datawheel",
    "olap-client",
    "plugin",
    "tesseract-explorer",
    "tesseract-olap"
  ],
  "author": "Francisco Abarzua <francisco@datawheel.us>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Datawheel/tesseract-ui.git",
    "directory": "packages/vizbuilder"
  },
  "dependencies": {
    "@datawheel/vizbuilder": "^0.5.0",
    "d3plus-react": "^1.0.0"
  },
  "peerDependencies": {
    "@datawheel/olap-client": "^2.0.0",
    "@datawheel/tesseract-explorer": "^2.0.0-0",
    "@tabler/icons-react": "^2.0.0",
    "@mantine/core": "^6.0.0",
    "react": "^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@datawheel/tesseract-explorer": "*",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "tsup": "^6.6.3"
  },
  "eslintConfig": {
    "env": {
      "browser": true
    },
    "globals": {
      "process": "readonly"
    }
  }
}
