{
  "name": "@datawheel/tesseract-vizbuilder",
  "version": "0.1.6",
  "description": "Generates a vizbuilder view for results query in Tesseract Explorer.",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "default": "./dist/index.cjs.js"
    },
    "./vizbuilder.css": "./dist/vizbuilder.css"
  },
  "types": "index.d.ts",
  "style": "dist/vizbuilder.css",
  "readme": "README.md",
  "files": [
    "index.d.ts",
    "dist/index.cjs.js",
    "dist/index.esm.js",
    "dist/vizbuilder.css"
  ],
  "scripts": {
    "build": "rollup --config ../../rollup.config.js --environment NODE_ENV:production",
    "watch": "rollup --config ../../rollup.config.js --environment NODE_ENV:development --watch"
  },
  "keywords": [
    "charts",
    "d3plus",
    "datawheel",
    "olap-client",
    "plugin",
    "tesseract-explorer",
    "tesseract-olap"
  ],
  "author": "Francisco Abarzua <francisco@datawheel.us>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Datawheel/tesseract-ui.git",
    "directory": "packages/vizbuilder"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.13.10",
    "@datawheel/vizbuilder": "^0.2.9"
  },
  "devDependencies": {
    "@datawheel/tesseract-explorer": "workspace:^1.0.0-alpha.10",
    "prop-types": "^15.6.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "rollup": "^2.45.2"
  },
  "peerDependencies": {
    "@datawheel/tesseract-explorer": "^1.0.0-alpha.0",
    "prop-types": ">=15.6.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  }
}
