    "@mantine/hooks": "^6.0.0",
    "@mantine/prism": "^6.0.0",
    "react": "^16.14.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.14.0 || ^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@emotion/react": "^11.10.6",
    "@mantine/core": "^6.0.0",
    "@mantine/dates": "^6.0.0",
    "@mantine/hooks": "^6.0.0",
    "@mantine/prism": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-config-tesseract-ui": "*",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "tsup": "^6.6.3"
  },
  "eslintConfig": {
    "env": {
      "browser": true
    },
    "globals": {
      "process": "readonly"
    }
  }
