{
  "name": "@datawheel/tesseract-explorer",
  "version": "1.0.0-alpha.30",
  "description": "A react component to explore data from a tesseract-olap server.",
  "main": "./dist/explorer.cjs.js",
  "module": "./dist/explorer.esm.js",
  "style": "./dist/explorer.css",
  "types": "./index.d.ts",
  "readme": "./README.md",
  "exports": {
    ".": {
      "import": "./dist/explorer.esm.js",
      "require": "./dist/explorer.cjs.js",
      "default": "./dist/explorer.cjs.js"
    },
    "./style.css": "./dist/explorer.css"
  },
  "files": [
    "dist/explorer.css",
    "dist/explorer.esm.js",
    "dist/explorer.cjs.js",
    "index.d.ts"
  ],
  "scripts": {
    "build": "rollup --config ../../rollup.config.js --environment NODE_ENV:production",
    "watch": "rollup --config ../../rollup.config.js --environment NODE_ENV:development --watch"
  },
  "author": "Francisco Abarz√∫a <francisco@datawheel.us>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Datawheel/tesseract-ui.git",
    "directory": "packages/tesseract-explorer"
  },
  "keywords": [
    "tesseract-explorer",
    "tesseract-olap",
    "tesseract-ui"
  ],
  "dependencies": {
    "@babel/runtime-corejs3": "^7.18.0",
    "@datawheel/olap-client": "^2.0.0",
    "@datawheel/use-translation": "^0.1.0",
    "classnames": "^2.3.0",
    "clipboard-copy": "^3.2.0",
    "d3plus-format": "^1.1.0",
    "form-urldecoded": "^1.2.0",
    "form-urlencoded": "^6.0.0",
    "iso-639-1": "^2.1.0",
    "react-shadow-scroll": "^2.1.0",
    "react-viewport-list": "^3.0.0",
    "reselect": "^4.1.0"
  },
  "devDependencies": {
    "@blueprintjs/core": "^3.54.0",
    "@blueprintjs/select": "^3.19.0",
    "@blueprintjs/table": "^3.10.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-redux": "^7.2.0",
    "redux": "^4.2.0",
    "rollup": "^2.75.0"
  },
  "peerDependencies": {
    "@blueprintjs/core": "^3.43.0",
    "@blueprintjs/select": "^3.15.0",
    "@blueprintjs/table": "^3.8.0",
    "react": "^16.13.0 || ^17.0.0",
    "react-dom": "^16.13.0 || ^17.0.0",
    "react-redux": "^7.0.0",
    "redux": "^4.0.0"
  }
}
