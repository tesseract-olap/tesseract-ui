      "default": "./dist/index.cjs.js"
    }
  },
  "files": [
    "dist/index.esm.js",
    "dist/index.cjs.js",
    "dist/index.d.ts"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "tsup"
  },
  "author": "Francisco Abarz√∫a <francisco@datawheel.us>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Datawheel/tesseract-ui.git",
    "directory": "packages/tesseract-explorer"
  },
  "keywords": [
    "tesseract-explorer",
    "tesseract-olap",
    "tesseract-ui"
  ],
  "dependencies": {
    "@datawheel/olap-client": "^2.0.0",
    "@datawheel/use-translation": "^0.2.0",
    "@reduxjs/toolkit": "^1.9.3",
    "@tabler/icons-react": "^2.7.0",
    "d3plus-format": "^1.1.0",
    "dayjs": "^1.0.0",
    "form-urldecoded": "^1.2.0",
    "form-urlencoded": "^6.0.0",
    "iso-639-1": "^2.1.0",
    "mantine-react-table": "^1.3.0",
    "react-redux": "^7.0.0",
    "react-viewport-list": "^3.0.0"
  },
  "peerDependencies": {
    "@emotion/react": "^11.0.0",
    "@mantine/core": "^6.0.0",
