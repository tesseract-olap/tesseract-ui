{
  "assumptions":{
    "noDocumentAll": true,
    "noClassCalls": true,
    "noNewArrows": true,
    "setSpreadProperties": true,
    "skipForOfIteratorClosing": true
  },
  "babelrcRoots": [
    ".",
    "example",
    "packages/*"
  ],
  "plugins": [
    ["@babel/plugin-transform-runtime", {
      "corejs": 3,
      "helpers": true,
      "regenerator": true,
      "version": "^7.12.1"
    }],
    ["@babel/plugin-proposal-class-properties", {}]
  ],
  "presets": [
    ["@babel/preset-env", {
      "useBuiltIns": false
    }],
    ["@babel/preset-react", {}]
  ]
}
